{
    "name": "sswikicrawler",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gilbitron/PHP-SimpleCache"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bobthecow/mustache.php"
        }
    ],
    "require": {
        "gilbitron/php-simplecache": "dev-master",
        "mustache/mustache": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "autoload": {
        "psr-4": { "sswikicrawler\\": "app/" }
    }
}